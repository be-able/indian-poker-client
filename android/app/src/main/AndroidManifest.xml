<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	package="kr.co.beable.indian.poker">

	<uses-permission android:name="android.permission.INTERNET" />

	<application
		android:name=".base.PokerApplication"
		android:allowBackup="true"
		android:icon="@drawable/icon_color"
		android:label="@string/app_name"
		android:roundIcon="@drawable/icon_color"
		android:supportsRtl="true"
		android:theme="@style/AppTheme"
		tools:ignore="AllowBackup,GoogleAppIndexingWarning">

		<activity android:name=".android.game.GameActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".android.splitter.SplitterActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

	</application>


</manifest>